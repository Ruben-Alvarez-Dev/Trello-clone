{
  "source": {
    "include": ["./src/", "./README.md"],
    "includePattern": "\\.(js|jsx)$",
    "exclude": ["node_modules/", "dist/", "src/assets/"]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "plugins": ["plugins/markdown"],
  "docdash": {
    "static": true,
    "sort": true,
    "search": true,
    "collapse": false,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [],
    "menu": {
      "Github": {
        "href": "https://github.com/Ruben-Alvarez-Dev/Trello-clone",
        "target": "_blank",
        "class": "menu-item",
        "id": "website_link"
      }
    },
    "meta": {
      "title": "Trello Clone Documentation",
      "description": "Complete documentation for the Trello Clone React application"
    }
  }
}
